@model Diesel_modular_application.Models.TableDieslovani
<div class="conteiner" id="con">
    <div class="tables-row" style="justify-content: space-between;">
        
        <div class="section small" style="flex: none; width: 30%; display:flex; align-items: center;" >
            <div class="detail diesel">
                <h1>Detail Dieslovani <i class="fa-solid fa-circle-info fa-lg"></i></h1>
                <div class="formINFO-D">
                    <div>
                        <p><i class="fa-solid fa-key fa-lg fa-sm navA"></i>ID dieslovani: @Model.IdDieslovani</p>
                    </div>
                    <div>
                        <p> <i class="fa-solid fa-info fa-lg fa-sm navA"></i> ID odstávky: @Model.Odstavka.IdOdstavky</p>
                    </div>
                    <div>
                        <p><i class="fa-solid fa-tower-broadcast fa-lg fa-sm navA"></i> Lokalita: @Model.Odstavka.Lokality.Lokalita</p>
                    </div>
                    <div>
                        <p><i class="fa-solid fa-location-dot fa-lg fa-sm navA"></i>Adresa: @Model.Odstavka.Lokality.Adresa </p>
                    </div>
                    <div>
                        <p><i class="fa-solid fa-info fa-lg fa-sm navA"></i> Klasifikace: @Model.Odstavka.Lokality.Klasifikace </p>
                    </div>
                    <div>              
                        <p><i class="fa-solid fa-battery-full fa-lg fa-sm navA"></i> Výdrž baterie: @Model.Odstavka.Lokality.Baterie min</p>
                    </div>
                    <div>
                        <p><i class="fa-solid fa-earth-europe fa-lg fa-sm navA"></i> Region: @Model.Odstavka?.Lokality?.Region?.NazevRegionu</p>
                    </div>
                    <div>
                        <p> <i class="fa-regular fa-circle-user fa-2xl fa-sm navA"></i> Technik: @Model.Technik?.Jmeno  @Model.Technik?.Prijmeni</p>
                    </div>
                </div>
            </div>

        </div>
        <div class="section small" style="flex: none; width: 70%;">
            <div class="detail energy">
                <h1>Detail Dieslovani <i class="fa-solid fa-circle-info fa-lg"></i></h1>

                <div class="infowrapper">
                    
                    <div class="info battery">
                        <h1>Battery info</h1>
                        <div style="padding: 10px">
                            <i class="fa-solid fa-battery-full fa-2xl"></i>
                        </div>
                        <div class="formInfo">
                            <div>
                                <p>battery float charge: 100%</p>
                            </div>
                            <div>
                                <p>Time remains: 125 min</p>
                            </div>
                            <div>
                                <p>Output voltage: 55V</p>
                            </div>
                            <div>
                                <p>Output Current: 17A</p>
                            </div>
                        </div>
                    

                    </div>
                    <div class="info dc">
                        <h1>DC info</h1>
                        <div style="padding: 10px">
                            <i class="fa-solid fa-power-off fa-2xl"></i>
                        </div>
                        <div class="formInfo">  
                            <div>
                                <p>L1: 230 V</p>
                            </div>
                            <div>    
                                <p>L2: 230 V</p>
                            </div>
                            <div>
                                <p>L3: 230 V</p>
                            </div>
                            <div>
                                <p>Product model: MB30B</p>
                            </div>
                        </div>
                    
                    </div>
                    <div class="info DA ">
                        <h1>DA info</h1>
                        <div style="padding: 10px"> 
                            <i class="fa-solid fa-gas-pump fa-2xl"></i>
                        </div>
                        <div class="formInfo"> 
                            <div>
                                <p>DA-IS-RUNINNG </p>
                            </div>
                            <div>
                                <p>DA-model: </p>
                            </div>
                            <div>
                                <p>Spotřeba: </p>
                            </div>
                        </div>
                       
                    </div>
                </div>
            </div>
        </div>   




        <div class="tables-row" style="width: 100%; justify-content: center;">
            <div class="table-div odstavkyTable">
                <div class="name">
                    <h2 style="padding-right:20px; color: black;">Odstávka<i class="fa-solid fa-bolt"></i></h2>
                    <div>
                        <span class="legenda">
                            Aktivní :
                            <i class="fa-solid fa-square" style="color: rgba(0, 128, 0, 0.371)"></i>
                        </span>
                        <span class="legenda">
                            Ukončené :
                            <i class="fa-solid fa-square" style="color: rgba(255, 0, 0, 0.382)"></i>
                        </span>
                        <span class="legenda">
                            Nekativni :
                            <i class="fa-solid fa-square" style="color: #0055ff28"></i>
                        </span>
                        <span class="legenda">
                            Nepřižazeno :
                            <i class="fa-solid fa-square" style="color:  rgba(255, 166, 0, 0.521"></i> 
                        </span>
                        <span class="legenda">
                            Čekající :
                            <i class="fa-solid fa-square" style="color: rgba(255, 255, 0, 0.321"></i>
                        </span>  
                    </div>
                </div>
                <table id="OdDetail" style="margin-bottom: 15px;">
                    <thead>
                        <tr>
                            <th>Akce</th>
                            <th>Id</th>
                            <th>Distributor</th>
                            <th>Lokalita</th>
                            <th>Klasifikace</th>
                            <th>Od</th>
                            <th>Do</th>
                            <th>Adresa</th>
                            <th>Baterie<a> [min]</a></th>
                            <th>Popis</th>
                            <th>Zásuvka</th>
                        </tr>
                    </thead>
                    <tbody id="table-body">     
                    </tbody>          
                </table>   
            </div>
                
            
        </div>
    </div>
</div>